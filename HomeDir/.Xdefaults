! To only apply colors to your terminal, for example, prefix
! the color assignment statement with its name. Example:
!
! URxvt*background:            S_base03

*background:              S_base03
*foreground:              S_base0
*fading:                  40
*fadeColor:               S_base03
*cursorColor:             S_base1
*pointerColorBackground:  S_base01
*pointerColorForeground:  S_base1

*color0:                  S_base02
*color1:                  S_red
*color2:                  S_green
*color3:                  S_yellow
*color4:                  S_blue
*color5:                  S_magenta
*color6:                  S_cyan
*color7:                  S_base2
*color9:                  S_orange
*color8:                  S_base03
*color10:                 S_base01
*color11:                 S_base00
*color12:                 S_base0
*color13:                 S_violet
*color14:                 S_base1
*color15:                 S_base3





!! urxvt stuff

URxvt.intensityStyles:	false
URxvt.background:       #000000
URxvt.depth:		32
URxvt.font: 		xft:monospace:size=11
URxvt.scrollBar:	true
URxvt.cursorColor:	magenta

!! Extensions resize-font, url-select,
URxvt.perl-ext-common:	default,matcher,keyboard-select,tabbed
URxvt.colorUL:		#4682B4
!! url-select
!! URxvt.keysym.M-u:	perl:url-select:select_next
!! URxvt.url-select.launcher: linkhandler
!! URxvt.url-select.underline: true
!! keyboard-select:
!! URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-u: perl:keyboard-select:activate
!! resize-font
!! URxvt.resize-font.smaller:	C-Down
!! URxvt.resize-font.bigger:	C-Up
!! Matcher
!! URxvt.url-launcher:	linkhandler
!! URxvt.matcher.button:	1

!! Copy and Paste
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

